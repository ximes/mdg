<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<name>MdG</name>
	<version>1.0.0</version>
	<requirements>
		<minimum_version>3.0.0</minimum_version>
	</requirements>
	<thumbnail>thumbnail.png</thumbnail>
	<description>
		<![CDATA[
			
		]]>
	</description>
	<authors>
		<author>
			<name>orion</name>
			<url>http://www.mucchedighisa.com</url>
		</author>
	</authors>
	<metanavigation supported="false" />
	<templates>
		<template label="Default" path="core/layout/templates/default.tpl">
			<positions>
				<position name="top" />
				<position name="main" />
				<position name="ribbonLeft" />
				<position name="ribbonRight" />
				<position name="contactLeft" />
				<position name="contactRight" />
				<position name="footer" />
			</positions>
			<format>
				[top,top],[main,main],[ribbonLeft,ribbonRight],[contactLeft,contactRight],[footer,footer]
			</format>
		</template>
		<template label="Blog" path="core/layout/templates/blog.tpl">
			<positions>
				<position name="top" />
				<position name="main" />
				<position name="right" />
				<position name="ribbonLeft" />
				<position name="ribbonRight" />
				<position name="contactLeft" />
				<position name="contactRight" />
				<position name="footer" />
			</positions>
			<format>
				[top,top,top,top],[main,main,main,right],[ribbonLeft, ribbonLeft,ribbonRight, ribbonRight],[contactLeft,contactLeft,contactRight,contactRight],[footer,footer,footer,footer]
			</format>
		</template>
	</templates>
</theme>